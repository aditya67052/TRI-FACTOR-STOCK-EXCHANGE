<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_billing"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.winindo1.smallmedium.Billing">
    <EditText
        android:layout_width="395dp"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:ems="10"
        android:id="@+id/name"
        android:hint="Name"
        android:textColorHint="#FF00ddFF"
        android:textColor="#FFFF4081"
        android:textSize="20sp"
        tools:layout_editor_absoluteY="29dp"
        tools:layout_editor_absoluteX="-5dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        tools:ignore="HardcodedText" />

    <EditText
        android:layout_width="395dp"
        android:layout_height="wrap_content"
        android:inputType="text"
        android:ems="10"
        android:id="@+id/mobile"
        android:hint="MobileNumber"
        android:textColorHint="#FF00ddFF"
        android:textColor="#FFFF4081"
        android:textSize="20sp"
        tools:layout_editor_absoluteY="102dp"
        android:layout_below="@+id/name"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="22dp"
        tools:ignore="HardcodedText" />

    <EditText
        android:layout_width="395dp"
        android:layout_height="wrap_content"
        android:inputType="textPostalAddress"
        android:ems="10"
        android:id="@+id/address"
        android:hint="Address"
        android:textColorHint="#FF00ddFF"
        android:textColor="#FFFF4081"
        android:textSize="20sp"
        tools:layout_editor_absoluteY="182dp"
        tools:layout_editor_absoluteX="-5dp"
        android:layout_marginTop="17dp"
        android:layout_below="@+id/mobile"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        tools:ignore="HardcodedText" />

    <EditText
        android:layout_width="395dp"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:ems="10"
        android:id="@+id/stat"
        android:hint="State"
        android:textColorHint="#FF00ddFF"
        android:textColor="#FFFF4081"
        android:textSize="20sp"
        tools:layout_editor_absoluteY="336dp"
        tools:layout_editor_absoluteX="8dp"
        android:layout_below="@+id/address"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="19dp"
        tools:ignore="HardcodedText" />

    <EditText
        android:layout_width="395dp"
        android:layout_height="wrap_content"
        android:inputType="text"
        android:ems="10"
        android:id="@+id/cont"
        android:hint="Country"
        android:textColorHint="#FF00ddFF"
        android:textColor="#FFFF4081"
        android:textSize="20sp"
        tools:layout_editor_absoluteY="407dp"
        tools:layout_editor_absoluteX="8dp"
        android:layout_marginTop="15dp"
        android:layout_below="@+id/stat"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/submit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Submit"
        android:textColor="#FF00ddFF"
        android:background="@drawable/round_button1"
        tools:layout_editor_absoluteX="25dp"
        tools:layout_editor_absoluteY="495dp"
        android:layout_marginBottom="29dp"
        android:layout_marginLeft="22dp"
        android:layout_marginStart="22dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        tools:ignore="HardcodedText" />
    <Button
        android:id="@+id/cancel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cancel"
        android:textColor="#ff09"
        android:background="@drawable/round_button"
        tools:layout_editor_absoluteX="293dp"
        tools:layout_editor_absoluteY="495dp"
        android:layout_marginRight="24dp"
        android:layout_marginEnd="24dp"
        android:layout_alignBaseline="@+id/submit"
        android:layout_alignBottom="@+id/submit"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        tools:ignore="ButtonOrder,HardcodedText" />


</RelativeLayout>
